
<section id="main-container" class="main-container">
	<div class="container">
		<div clas="row">
			<div class="col s10 push-s1">
				<div class="row">
					
					<div class="col s12 m7">
						<div class="row">
	
                                <h3 id="center">Visualización de cuestionarios</h3>

                                	<div id="quizz" class="quizz">
                                        {{#each quizz}}
                                        <ul class="collection with-header">
                                            <li class="collection-header">
                                               <h6><div>{{nom_cue}}<a href="#!" class="secondary-content"><i class="material-icons">create</i></a></div></h6> 
                                            </li>
                                            <li class="collection-item"><div>Fecha de Creación: {{fec_ini}}</div></li>
                                            <li class="collection-item"><div>Fecha de Fin: {{fec_fin}}</div></li>
                                            {{#each questions}}
                                            <li class="collection-item"><div><span class="mathquillEstatic">{{con_pre}}</span></div></li>
                                            
                                            {{/each}}
                                        </ul>
                                        {{/each}}
                                    </div>
					</div>
				</div>  
			</div>
		</div>
	</div>
  </div>

</section>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
<script src="/mathquill/mathquill.js"></script>
<script>
var MQ = MathQuill.getInterface(2); // for backcompat

let contenidoPregunta = document.getElementsByClassName('mathquillEstatic');
for(let index = 0; index < contenidoPregunta.length; index++) {
    MQ.StaticMath(contenidoPregunta.item(index));
}
let esconder = document.getElementsByClassName('mq-selectable');
for(let index = 0; index < esconder.length; index++) {
    esconder.item(index).innerHTML = "";
}
</script>